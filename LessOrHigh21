public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
		int[] arr={1,4,6,22,45,67,8,20};
		int m1=0;
		int m2=0;
		for(int i=0;i<arr.length;i++){
		    if(arr[i]<=21){
		        m1++;
		    }
		    else{
		        m2++;
		    }
		}
		System.out.println(m1+" "+ m2);
	}
}
