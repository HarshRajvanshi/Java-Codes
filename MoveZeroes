//https://leetcode.com/problems/move-zeroes/submissions/
class Solution {
    public void moveZeroes(int[] nums) {
        int index=0;
        if(nums.length>1){
        for(int i=0;i<nums.length;i++){
            if(nums[i]!=0){
                nums[index]=nums[i];
                index++;
                if(i>=index){
                    nums[i]=0;
                }
                
            }
            
        }
        }       
    }
}
