class Main {
  public static void main(String[] args) {
    
    int a = 10;
    int b=100;
    int temp=a;
    System.out.println(a+" "+b + "  original values");
    System.out.println("Using third variable");
    a=b;
    b=temp;
    System.out.println(a+" "+b);
    System.out.println("without third variable ");
    a=a+b;
    b=a-b;
    a=a-b;
    System.out.println(a+" "+b);
  }
}
