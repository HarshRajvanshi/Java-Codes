
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	 {
	     Scanner in = new Scanner(System.in);
	     String str=in.nextLine();
		String res="";
		for(int i=0;i<str.length();i++){
			char x=str.charAt(i);
			if(x=='a' || x=='e' || x=='i' ||x=='o' || x=='u'){
				continue;
			}
			else{
				res+=x;
			}
		}
		System.out.println(res);
	}
}
}
